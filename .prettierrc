{
  "arrowParens": "always",
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "semi": false,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(^react$|@react|react)",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
